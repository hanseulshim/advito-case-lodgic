{
	"name": "advito-case-lodgic",
	"version": "1.0.0",
	"description": "Serverless webpack example using Typescript",
	"main": "handler.js",
	"scripts": {
		"start": "nodemon -e js,ts,yml --ignore node_modules --exec \"sls offline -s local\"",
		"start:dev": "nodemon -e js,ts,yml --ignore node_modules --exec \"sls offline -s dev\"",
		"start:staging": "nodemon -e js,ts,yml --ignore node_modules --exec \"sls offline -s staging\"",
		"start:production": "nodemon -e js,ts,yml --ignore node_modules --exec \"sls offline -s production\"",
		"deploy:staging": "sls deploy -s staging --aws-profile advito",
		"deploy:dev": "sls deploy -s dev --aws-profile advito",
		"deploy:production": "sls deploy -s production --aws-profile advito",
		"db:seed:dev": "node_modules/.bin/knex seed:run --knexfile=./db/knexfile.js",
		"db:seed:staging": "node_modules/.bin/knex seed:run --knexfile=./db/knexfile.js --env staging"
	},
	"dependencies": {
		"apollo-server-lambda": "^2.11.0",
		"aws-sdk": "^2.645.0",
		"faker": "^4.1.0",
		"graphql": "^14.6.0",
		"knex": "^0.20.13",
		"lodash.merge": "^4.6.2",
		"moment": "^2.24.0",
		"objection": "^2.1.3",
		"pg": "^7.18.2",
		"source-map-support": "^0.5.10",
		"yenv": "^2.1.1"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.17",
		"@types/faker": "^4.1.11",
		"@types/lodash.merge": "^4.6.6",
		"@types/node": "^10.12.18",
		"@typescript-eslint/eslint-plugin": "^2.24.0",
		"@typescript-eslint/parser": "^2.24.0",
		"eslint": "^6.8.0",
		"fork-ts-checker-webpack-plugin": "^3.0.1",
		"nodemon": "^2.0.2",
		"serverless-offline": "^6.0.0-alpha.67",
		"serverless-webpack": "^5.2.0",
		"ts-loader": "^5.3.3",
		"typescript": "^3.2.4",
		"webpack": "^4.29.0",
		"webpack-node-externals": "^1.7.2"
	},
	"author": "boost",
	"license": "MIT",
	"repository": "git@github.com:boostlabscom/advito-case-lodgic.git"
}
